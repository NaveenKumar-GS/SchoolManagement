
@model IEnumerable<Schoolmanagementn.Attendance>

@{
    ViewBag.Title = "Index";
}

<table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.DateTime)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Attendance1)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.UserRegistration.FullName)
            </th>
            <th>Options</th>
        </tr>

    @foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.DateTime)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Attendance1)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.UserRegistration.FullName)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id=item.AttendenceId }) |
                @Html.ActionLink("Details", "Details", new { id=item.AttendenceId }) |
                @Html.ActionLink("Delete", "Delete", new { id=item.AttendenceId })
            </td>
        </tr>
    }

    </table>










